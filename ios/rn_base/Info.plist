<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<!-- Cấu hình tương ứng hiển thị Xcode-->
		<key>CFBundleDevelopmentRegion</key>
		<string>en</string>
		<key>CFBundleDisplayName</key>
		<string>rn_base</string>
		<key>CFBundleExecutable</key>
		<string>$(EXECUTABLE_NAME)</string>
		<key>CFBundleIdentifier</key>
		<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
		<key>CFBundleInfoDictionaryVersion</key>
		<string>6.0</string>
		<key>CFBundleName</key>
		<string>$(PRODUCT_NAME)</string>
		<key>CFBundlePackageType</key>
		<string>APPL</string>
		<key>CFBundleShortVersionString</key>
		<string>$(MARKETING_VERSION)</string>
		<key>CFBundleSignature</key>
		<string>????</string>
		<key>CFBundleVersion</key>
		<string>$(CURRENT_PROJECT_VERSION)</string>

		<!--File đâu tiên được chạy để lấy ảnh nền ứng dụng(giá trị string)-->
		<key>UILaunchStoryboardName</key>
		<string>LaunchScreen</string>

		<!--Các tính năng liên quan đến thiết bị mà ứng dụng của bạn yêu cầu để chạy(giá trị là mảng)-->
		<key>UIRequiredDeviceCapabilities</key>
		<array>
			<string>armv7</string>
		</array>

		<!--Các hướng giao diện được ứng dụng của bạn hỗ trợ.(giá trị là mảng)-->
		<key>UISupportedInterfaceOrientations</key>
		<array>
			<string>UIInterfaceOrientationPortrait</string>
			<string>UIInterfaceOrientationLandscapeLeft</string>
			<string>UIInterfaceOrientationLandscapeRight</string>
		</array>

		<!--Cho biết liệu giao diện thanh trạng thái có dựa trên kiểu được ưu tiên cho bộ điều khiển chế độ xem hiện tại hay không.(giá trị là Boolean)-->
		<key>UIViewControllerBasedStatusBarAppearance</key>
		<false/>

		<!-- Key Code Push-->
		<key>CodePushDeploymentKey</key>
		<string>6he918zK2LBUBdDiMVoXp-9s99gqLks4MFViy</string>

		<!-- Auto Missing-->
		<key>ITSAppUsesNonExemptEncryption</key>
		<false/>

		<!--Yêu cầu ứng dụng có bắt buộc chạy trên hệ điều hành IOS hay không?-->
		<key>LSRequiresIPhoneOS</key>
		<true/>

		<!--Mô tả các thay đổi được thực hiện đối với bảo mật mặc định cho các kết nối HTTP.-->
		<key>NSAppTransportSecurity</key>
		<dict>
			<key>NSAllowsArbitraryLoads</key>
			<true/>
			<key>NSExceptionAllowsInsecureHTTPLoads</key>
			<true/>
			<key>NSExceptionDomains</key>
			<dict>
				<key>localhost</key>
				<dict>
					<key>NSExceptionAllowsInsecureHTTPLoads</key>
					<true/>
				</dict>
			</dict>
			<!-- Các cấu hình tùy chỉnh cho các miền có tên App Transport Security.-->
			<key>NSExceptionMinimumTLSVersion</key>
			<string>TLSv1.2</string>
			<key>NSExceptionRequiresForwardSecrecy</key>
			<true/>
			<key>NSIncludesSubdomains</key>
			<true/>
			<key>NSRequiresCertificateTransparency</key>
			<false/>
			<key>NSThirdPartyExceptionAllowsInsecureHTTPLoads</key>
			<false/>
			<key>NSThirdPartyExceptionMinimumTLSVersion</key>
			<string>TLSv1.2</string>
			<key>NSThirdPartyExceptionRequiresForwardSecrecy</key>
			<true/>
		</dict>

		<!--Danh sách quyền ứng dụng-->
		<!--Quyền Camera-->
		<key>NSCameraUsageDescription</key>
		<string>Xin quyền Camera.</string>
		<!--Quyền vị trí IOS 13 trở lên có 3 lựa chọn tương ứng-->
		<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
		<string>Xin quyền vị trí.</string>
		<key>NSLocationAlwaysUsageDescription</key>
		<string>Xin quyền vị trí.</string>
		<key>NSLocationWhenInUseUsageDescription</key>
		<string>Xin quyền vị trí.</string>
		<!--Quyền truy cập vào thư mục ảnh đọc và ghi-->
		<key>NSPhotoLibraryAddUsageDescription</key>
		<string>Xin quyền truy cập vào thư mục ảnh</string>
		<key>NSPhotoLibraryUsageDescription</key>
		<string>Xin quyền truy cập vào thư mục ảnh</string>

